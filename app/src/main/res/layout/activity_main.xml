<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="219dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="319dp"
        android:layout_height="87dp"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="28dp"
        android:gravity="center_horizontal"
        android:text="Google's Code Next program works with high schoolers to cultivate the next generation of Black and Latinx tech leaders."
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <ImageButton
        android:id="@+id/menu"
        android:layout_width="41dp"
        android:layout_height="44dp"
        android:background="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/drop" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="403dp"
        android:layout_height="520dp"
        android:layout_marginTop="172dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/coaches"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="Coaches" />

        <Button
            android:id="@+id/programs"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="Programs" />

        <Button
            android:id="@+id/locations"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="Locations" />

        <Button
            android:id="@+id/faq"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:text="FAQ" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>